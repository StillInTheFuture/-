.share{
  width: 310px;
  height: 200px;
}
form{
  margin-top:30px;
  width: 310px;
}
.section{
  margin-top: 10px;
  width:100%;
  height:39px;
  display:inline-block;
  background:#fff;
  border-bottom: 1px solid #e1e1e1;
}
.section view{
  width:20%;
  float: left;
  height:100%;
  text-align: center;
}
.section view image{
  width: 20px;
  height: 20px;
  margin-top: 9px;
}
.section input{
  width:80%;
  display:inline-block;
  height:100%;
  font-size: 16px;
}
.button{
  margin-top: 30px;
}
.button button{
  background: #4877ed;
}
.note{
  font-size: 14px;
  margin-top: 10px;
  width: 310px;
}
.note navigator:first-child{
  float: left;
  margin-left: 10px;
}
.note navigator:last-child{
  float: right;
  margin-right: 10px;
}
.navigator-hover{
  color: #4877ed;
  background: none;
}

